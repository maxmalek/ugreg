{"devicetypes":{
    "smp351": {
        "heartbeat_time": "30s",
        "errors": {
            "E10": "Unrecognized command",
            "E12": "Invalid port number",
            "E13": "Invalid parameter (number is out of range)",
            "E14": "Not valid for this configuration",
            "E17": "Invalid command for signal type",
            "E18": "System timed out",
            "E22": "Busy",
            "E24": "Privilege violation",
            "E25": "Device is not present",
            "E26": "Maximum connections exceeded",
            "E28": "Bad file name or file not found",
        },
        "actions": {
            "login": [
                ["need", 32],
                ["match", "\\s*\\(c\\) Copyright [0-9\\-]+, Extron Electronics, SMP 35[12][^\\n]*\\n[^\\n]*\\s+"],
                ["need", 9],
                ["wait", "100ms"],
                ["match", "\\s*"],
                ["expect", "Password:"],
                ["send", "${./password}\n"],
                ["need", 5],
                ["match", "\\s*Login Administrator\\s*"],
            ],
            "heartbeat": [
                ["send", "Q"],
                ["need", 1],
                ["skipall"],
            ],
            "start": [
                ["skipall"],
                ["send", "xxx"],
                ["expect", "xxx"],
            ],
            "stop": [
                ["skipall"],
                ["send", "xxx"],
                ["expect", "xxx"],
            ],
            "pause": [
                ["skipall"],
                ["send", "xxx"],
                ["expect", "xxx"],
            ],
            "continue": [
                ["skipall"],
                ["send", "xxx"],
                ["expect", "xxx"],
            ],
            "status": [
                ["skipall"],
                ["send", "xxx"],
                //["read", "xxx"],
                ["need", 1],
                ["skipall"],
            ],
        },
    },
    "smp351-web": {
